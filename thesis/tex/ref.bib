@article{esmaili2013a,
  title = {Restricting the {{LSND}} and {{MiniBooNE}} Sterile Neutrinos with the {{IceCube}} Atmospheric Neutrino Data},
  author = {Esmaili, Arman and Smirnov, Alexei Yu},
  date = {2013-12},
  journaltitle = {J. High Energ. Phys.},
  volume = {2013},
  number = {12},
  eprint = {1307.6824},
  eprinttype = {arxiv},
  pages = {14},
  issn = {1029-8479},
  doi = {10.1007/JHEP12(2013)014},
  abstract = {We study oscillations of the high energy atmospheric neutrinos in the Earth into sterile neutrinos with the eV-scale mass. The MSW resonance and parametric enhancement of the \$\textbackslash bar\{\textbackslash nu\}\_\textbackslash mu\textbackslash to\textbackslash bar\{\textbackslash nu\}\_s\$ oscillations lead to distortion of the zenith angle distribution of the muon-track events which can be observed by IceCube. Due to matter effect, the IceCube signal depends not only on the mixing element \$U\_\{\textbackslash mu 4\}\$ relevant for LSND and MiniBooNE but also on \$U\_\{\textbackslash tau 4\}\$ and the CP-violating phase \$\textbackslash delta\_\{24\}\$. We show that the case with \$U\_\{\textbackslash tau 4\} = \textbackslash delta\_\{24\} = 0\$ leads to the weakest IceCube signal and therefore should be used to bound \$U\_\{\textbackslash mu 4\}\$. We compute the zenith angle distributions of the \$\textbackslash nu\_\textbackslash mu-\$events for different energy intervals in the range (0.1 - 10) TeV and find that inclusion of the energy information (binning in energy) improves the sensitivity to \$\textbackslash nu\_s\$ drastically. We estimate that with already collected (during 3 - 4 years) IceCube statistics the bound \$|U\_\{\textbackslash mu 4\}|\^2 {$<$} 0.01\$ (\$99\textbackslash\%\$ C.L.) can be established and the mixing required by LSND and MiniBooNE can be excluded at \$(4 - 6) \textbackslash sigma\$ confidence level.},
