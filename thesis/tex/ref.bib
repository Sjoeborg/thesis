@article{choubey2006,
  title = {Probing the Deviation from Maximal Mixing of Atmospheric Neutrinos},
  author = {Choubey, Sandhya and Roy, Probir},
  date = {2006-01-17},
  journaltitle = {Phys. Rev. D},
  volume = {73},
