  primaryclass = {astro-ph}
}

@article{gariazzoUpdatedGlobalAnalysis2017,
  title = {Updated {{Global}} 3+1 {{Analysis}} of {{Short}}-{{BaseLine Neutrino Oscillations}}},
  author = {Gariazzo, S. and Giunti, C. and Laveder, M. and Li, Y. F.},
  date = {2017-06},
  journaltitle = {J. High Energ. Phys.},
  volume = {2017},
  pages = {135},
  issn = {1029-8479},
  doi = {10.1007/JHEP06(2017)135},
  abstract = {We present the results of an updated fit of short-baseline neutrino oscillation data in the framework of 3+1 active-sterile neutrino mixing. We first consider \$\textbackslash nu\_e\$ and \$\textbackslash bar\textbackslash nu\_e\$ disappearance in the light of the Gallium and reactor anomalies. We discuss the implications of the recent measurement of the reactor \$\textbackslash bar\textbackslash nu\_e\$ spectrum in the NEOS experiment, which shifts the allowed regions of the parameter space towards smaller values of \$|U\_\{e4\}|\^2\$. The beta-decay constraints allow us to limit the oscillation length between about 2 cm and 7 m at \$3\textbackslash sigma\$ for neutrinos with an energy of 1 MeV. We then consider the global fit of the data in the light of the LSND anomaly, taking into account the constraints from \$\textbackslash nu\_e\$ and \$\textbackslash nu\_\textbackslash mu\$ disappearance experiments, including the recent data of the MINOS and IceCube experiments. The combination of the NEOS constraints on \$|U\_\{e4\}|\^2\$ and the MINOS and IceCube constraints on \$|U\_\{\textbackslash mu4\}|\^2\$ lead to an unacceptable appearance-disappearance tension which becomes tolerable only in a pragmatic fit which neglects the MiniBooNE low-energy anomaly. The minimization of the global \$\textbackslash chi\^2\$ in the space of the four mixing parameters \$\textbackslash Delta\{m\}\^2\_\{41\}\$, \$|U\_\{e4\}|\^2\$, \$|U\_\{\textbackslash mu4\}|\^2\$, and \$|U\_\{\textbackslash tau4\}|\^2\$ leads to three allowed regions with narrow \$\textbackslash Delta\{m\}\^\{2\}\_\{41\}\$ widths at \$ \textbackslash Delta m\^2\_\{41\} \textbackslash approx 1.7 \$ (best-fit), 1.3 (at \$2\textbackslash sigma\$), 2.4 (at \$3\textbackslash sigma\$) eV\$\^2\$. The restrictions of the allowed regions of the mixing parameters with respect to our previous global fits are mainly due to the NEOS constraints. We present a comparison of the allowed regions of the mixing parameters with the sensitivities of ongoing experiments, which show that it is likely that these experiments will determine in a definitive way if the reactor, Gallium and LSND anomalies are due to active-sterile neutrino oscillations or not.},
  archiveprefix = {arXiv},
  eprint = {1703.00860},
  eprinttype = {arxiv},
  file = {C\:\\Users\\Martin\\Zotero\\storage\\TLKYFFVA\\Gariazzo et al. - 2017 - Updated Global 3+1 Analysis of Short-BaseLine Neut.pdf;C\:\\Users\\Martin\\Zotero\\storage\\JBLNVH9N\\1703.html},
  keywords = {High Energy Physics - Experiment,High Energy Physics - Phenomenology,IC},
  number = {6}
}

