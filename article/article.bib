@online{axaniSterileNeutrinoSearches2020,
  title = {Sterile {{Neutrino Searches}} at the {{IceCube Neutrino Observatory}}},
  author = {Axani, Spencer},
  date = {2020-03-04},
  shortjournal = {ArXiv200302796 Hep-Ex Physicsphysics},
  url = {http://arxiv.org/abs/2003.02796},
  urldate = {2021-02-04},
  abstract = {The IceCube Neutrino Observatory is capable of performing a unique search for sterile neutrinos through the exploitation of a matter enhanced resonant neutrino oscillation phenomena. As atmospheric muon neutrinos pass the dense material within the Earth, neutral current elastic forward scattering is predicted to induce a transition into a sterile state. This thesis presents two 3+1 sterile neutrino analyses by searching for spectral differences in the reconstructed energy and zenith direction of muon neutrino events, indicative of a transition into a sterile state. The first search probes the parameter space \$\textbackslash Delta\$m\$\^2\_\{41\}\$ and sin\$\^2\$(2\$\textbackslash theta\_\{24\}\$) with relevant sensitivity to the global best fit region for a 3+1 sterile neutrino hypothesis. The second search performs a scan through sin\$\^2\$(2\$\textbackslash theta\_\{24\}\$) and sin\$\^2\$(2\$\textbackslash theta\_\{34\}\$) in the oscillation averaged out region of high-\$\textbackslash Delta\$m\$\^2\_\{41\}\$ (\$\textbackslash Delta\$m\$\^2\_\{41\}\$ \$\textbackslash gtrsim\$ 10eV\$\^2\$). The analyses are performed using an improved event selection, which was found to extract 305,891 well reconstructed muon neutrino events with a sample purity above 99.9\textbackslash\%, from eight years of IceCube data. Novel simulation techniques, along with updated calibration, and a re-assessment of the systematic uncertainties are also discussed. The first analysis finds a best fit sterile hypothesis point at \$\textbackslash Delta\$m\$\^2\_\{41\}\$ = 4.47eV\$\^2\$ and sin\$\^2\$(\$\textbackslash theta\_\{24\})\$ =0.10, consistent with the no-sterile hypothesis at the 8\textbackslash\% confidence level. The second analysis finds a best fit sterile hypothesis at sin\$\^2\$(\$\textbackslash theta\_\{34\})\$ = 0.40, sin\$\^2\$(\$\textbackslash theta\_\{24\})\$ = 0.006, consistent with the null hypothesis at the 19\textbackslash\% confidence level. \% and provides a significant improvement in the worlds measurements using muon neutrino disappearance.},
  archiveprefix = {arXiv},
  eprint = {2003.02796},
  eprinttype = {arxiv},
  file = {C\:\\Users\\Martin\\Zotero\\storage\\WLNNZ5QV\\Axani - 2020 - Sterile Neutrino Searches at the IceCube Neutrino .pdf;C\:\\Users\\Martin\\Zotero\\storage\\W9CGNDWX\\2003.html},
  keywords = {High Energy Physics - Experiment,Physics - Instrumentation and Detectors},
  primaryclass = {hep-ex, physics:physics}
}

