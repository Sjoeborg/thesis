@article{esmailiProbingNonStandardInteraction2013,
  title = {Probing {{Non}}-{{Standard Interaction}} of {{Neutrinos}} with {{IceCube}} and {{DeepCore}}},
  author = {Esmaili, Arman and Smirnov, Alexei Yu},
  date = {2013-06},
  journaltitle = {J. High Energ. Phys.},
  volume = {2013},
  pages = {26},
  issn = {1029-8479},
  doi = {10.1007/JHEP06(2013)026},
  abstract = {We consider effects of the Non-Standard Interactions (NSI) on oscillations of the high energy atmospheric neutrinos. The \textbackslash nu\_\textbackslash mu-oscillograms are constructed and their dependence on the NSI strength parameters \textbackslash epsilon\_\{\textbackslash alpha \textbackslash beta\} studied. We computed zenith angle distributions of the \textbackslash nu\_\textbackslash mu-events in the presence of NSI in different energy regions. The distributions are confronted with the IceCube-79 (high energy sample) and the DeepCore (low energy sample) data and constraints on the strength parameters |\textbackslash epsilon\_\{\textbackslash mu \textbackslash tau\}| {$<$} 6x10\^-3 and |\textbackslash epsilon\_\{\textbackslash mu\textbackslash mu\}-\textbackslash epsilon\_\{\textbackslash tau\textbackslash tau\}| {$<$} 3x10\^-2 (90\% C.L.) have been obtained. Future measurements of the zenith angle distributions by DeepCore in smaller energy bins will allow to improve the bounds by factor 2 - 3. We discuss the signatures of NSI in zenith angle and energy distributions of events which allow to discriminate them from the effects of sterile neutrinos.},
  archiveprefix = {arXiv},
  eprint = {1304.1042},
  eprinttype = {arxiv},
  file = {C\:\\Users\\Martin\\Zotero\\storage\\I329XTV5\\Esmaili and Smirnov - 2013 - Probing Non-Standard Interaction of Neutrinos with.pdf;C\:\\Users\\Martin\\Zotero\\storage\\J7FC88SR\\Esmaili and Smirnov - 2013 - Probing Non-Standard Interaction of Neutrinos with.html},
  keywords = {High Energy Physics - Experiment,High Energy Physics - Phenomenology},
  number = {6}
}

