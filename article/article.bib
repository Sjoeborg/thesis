}

@online{karagiorgiConfrontingShortbaselineOscillation2012,
  title = {Confronting the Short-Baseline Oscillation Anomalies with a Single Sterile Neutrino and Non-Standard Matter Effects},
  author = {Karagiorgi, G. and Shaevitz, M. H. and Conrad, J. M.},
  date = {2012-02-05},
  shortjournal = {ArXiv12021024 Hep-Ph},
  url = {http://arxiv.org/abs/1202.1024},
  urldate = {2020-09-21},
  abstract = {We examine the MiniBooNE neutrino, MiniBooNE antineutrino and LSND antineutrino data sets in a two-neutrino \$\textbackslash stackrel\{\textbackslash tiny\{(-)\}\}\{\textbackslash nu\}\_\{\textbackslash mu\}\textbackslash rightarrow\textbackslash stackrel\{\textbackslash tiny\{(-)\}\}\{\textbackslash nu\}\_e\$ oscillation approximation subject to non-standard matter effects. We assume those effects can be parametrized by an \$L\$-independent effective potential, \$V\_s=\textbackslash pm A\_s\$, experienced only by an intermediate, non-weakly-interacting (sterile) neutrino state which we assume participates in the oscillation, where \$+/-\$ corresponds to neutrino/antineutrino propagation. We discuss the mathematical framework in which such oscillations arise in detail, and derive the relevant oscillation probability as a function of the vacuum oscillation parameters \$\textbackslash Delta m\^2\$ and \$\textbackslash sin\^22\textbackslash theta\_\{\textbackslash mu e\}\$, and the matter effect parameter \$A\_s\$. We are able to successfully fit all three data sets, including the MiniBooNE low energy excess, with the following best-fit model parameters: \$\textbackslash Delta m\^2=0.47\$ eV\$\^2\$, \$\textbackslash sin\^22\textbackslash theta\_\{\textbackslash mu e\}=0.010\$, and \$A\_s=2.0\textbackslash times10\^\{-10\}\$ eV. The \$\textbackslash chi\^2\$-probability for the best fit corresponds to 21.6\%, to be compared to 6.8\% for a fit where \$A\_s\$ has been set to zero, corresponding to a (3+1) sterile neutrino oscillation model. We find that the compatibility between the three data sets corresponds to 17.4\%, to be compared to 2.3\% for \$A\_s=0\$. Finally, given the fit results, we examine consequences for reactor, solar, and atmospheric oscillations. For this paper, the presented model is empirically driven, but the results obtained can be directly used to investigate various phenomenological interpretations such as non-standard matter effects.},
  archiveprefix = {arXiv},
  eprint = {1202.1024},
  eprinttype = {arxiv},
  file = {C\:\\Users\\Martin\\Zotero\\storage\\ZVLQVBCY\\Karagiorgi et al. - 2012 - Confronting the short-baseline oscillation anomali.pdf;C\:\\Users\\Martin\\Zotero\\storage\\KI7J37P6\\1202.html},
  keywords = {High Energy Physics - Phenomenology,matter},
  primaryclass = {hep-ph}
}

